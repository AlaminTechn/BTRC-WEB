# BTRC QoS Monitoring - Environment Variables
# Copy this file to .env and fill in the values

# ===========================================
# BTRC Database (TimescaleDB)
# ===========================================
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_DB=
POSTGRES_HOST=
POSTGRES_PORT=
POSTGRES_EXTERNAL_PORT=5434

# ===========================================
# Superset Configuration
# ===========================================
SUPERSET_SECRET_KEY=your-secret-key-here-change-in-production
SUPERSET_ADMIN_USERNAME=admin
SUPERSET_ADMIN_PASSWORD=admin123
SUPERSET_ADMIN_EMAIL=admin@btrc.gov.bd

# ===========================================
# Superset Database (Metadata)
# ===========================================
SUPERSET_DB_USER=superset
SUPERSET_DB_PASSWORD=superset
SUPERSET_DB_NAME=superset

# ===========================================
# Redis Configuration
# ===========================================
REDIS_URL=redis://redis:6379/0

# ===========================================
# Python Version
# ===========================================
PYTHON_VERSION=3.10
